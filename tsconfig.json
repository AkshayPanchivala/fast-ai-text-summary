{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "outDir": "./dist/cjs",
    "declarationDir": "./dist/cjs",
    "declaration": true
  },
  "rootDir": "./src",
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "testcase"
  ]
}